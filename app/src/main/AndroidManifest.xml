<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.smartcity_test3">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.SmartCity_test3"
        android:usesCleartextTraffic="true">
        <activity android:name=".ui.personal.activity.PersonalInfoActivity" android:parentActivityName=".MainActivity" android:label="个人信息详情"></activity>
        <activity android:name=".ui.personal.activity.LoginActivity" android:label="登陆页面" android:parentActivityName=".MainActivity"/>
        <activity android:name=".ui.personal.activity.OrderActivity" android:parentActivityName=".MainActivity" android:label="订单列表页面"/>
        <activity android:name=".ui.personal.activity.FeedbackActivity" android:label="意见反馈页面" android:parentActivityName=".MainActivity"/>
        <activity android:name=".ui.personal.activity.ChangePasswordActivity" android:parentActivityName=".MainActivity" android:label="修改密码页面"/>
        <activity
            android:name=".parkingLot.ParkJiLuActivity"
            android:label="停车记录"
            android:parentActivityName=".parkingLot.ParkingActivity" />
        <activity
            android:name=".parkingLot.ParkingInfoActivity"
            android:label="停车场详情"
            android:parentActivityName=".parkingLot.ParkingActivity" />
        <activity
            android:name=".parkingLot.ParkingActivity"
            android:label="停车场功能模块"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>